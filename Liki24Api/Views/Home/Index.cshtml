

@{
    ViewBag.Title = "Liki24-WindowManager Web API";

}

<link type="text/css" href="~/Areas/HelpPage/HelpPage.css" rel="stylesheet" />
<header class="help-page">
    <div class="content-wrapper">
        <div class="float-left">
            <h1>@ViewBag.Title</h1>
        </div>
    </div>
</header>
<h2>TO START WORKING, OPEN ONE OF LINKS IN THE TABLE BELOW:</h2>
<div id="body" class="help-page">
    <section class="content-wrapper main-content clear-fix">
        <table class="help-page-table">
            <thead>
                <tr><th>Endpoint</th><th>Description</th></tr>
            </thead>
            <tbody>
                <tr>
                   <td class="api-name"><a href=@Url.Action("GetWindows", "Home")>GET api/GetWindows</a></td>
                    <td class="api-documentation">
                        <p>Get all windows in determined time-range.</p>
                    </td>
                </tr>
                <tr>
                    <td class="api-name"><a href=@Url.Action("CreateWindow", "Home")>POST api/CreateWindow</a></td>
                    <td class="api-documentation">
                        <p>Create new time window.</p>
                    </td>
                </tr>
            </tbody>
        </table>
    </section>
</div>
